 s = input("Enter a string: ")

found = False
for i in range(len(s)):
    count = 0
    for j in range(len(s)):
        if s[i] == s[j]:
            count += 1
    if count == 1:
        print("First non-repeating character:", s[i])
        found = True
        break

if not found:
    print("No non-repeating character")
